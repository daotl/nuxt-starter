{
  "name": "@daotl/nuxt-starter",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "description": "A Nuxt 3 + TypeScript starter template",
  "homepage": "https://github.com/daotl/nuxt-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/daotl/nuxt-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/nuxt-starter/issues"
  },
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "generate": "nuxi generate",
    "prepare": "nuxi prepare",
    "start": "node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "typecheck": "vue-tsc --noEmit",
    "cz": "npx cz"
  },
  "config": {
    "commitizen": {
      "path": "commitiquette"
    }
  },
  "dependencies": {
    "@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge@8.0.0-rc.4-28251471.09ead69",
    "element-plus": "^2.8.6",
    "pinia": "^2.2.4",
    "rxjs": "^7.8.1",
    "vue": "^3.5.12",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@daotl/eslint-config": "^4.6.1",
    "@element-plus/nuxt": "^1.0.10",
    "@iconify-json/carbon": "^1.2.8",
    "@iconify-json/twemoji": "^1.2.2",
    "@nuxt/devtools": "^2.2.0",
    "@nuxt/eslint": "^1.1.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "^0.10.1",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^20.16.14",
    "@unocss/eslint-config": "^66.1.0-beta.3",
    "@unocss/nuxt": "^66.1.0-beta.3",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vueuse/nuxt": "^12.8.0",
    "commitiquette": "^1.2.1",
    "commitizen": "^4.3.1",
    "consola": "^3.4.0",
    "eslint": "^9.21.0",
    "eslint-plugin-format": "^1.0.1",
    "lefthook": "^1.7.22",
    "nuxt": "^3.15.4",
    "nuxt-typed-router": "^3.7.0",
    "pinia": "^3.0.1",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-syntax": "^0.36.2",
    "sass": "^1.80.3",
    "stylelint": "^16.10.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "^5.8.2",
    "unplugin-auto-import": "^19.1.1",
    "vue-tsc": "^2.2.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  },
  "resolutions": {
    "@nuxt/devtools": "^2.2.0",
    "unplugin": "^2.2.0",
    "vite": "^6.2.0"
  }
}
